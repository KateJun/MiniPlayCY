.tp_progress_cover {
  position: fixed;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.tp_progress_cover_bg {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: -1;
}

.tp_progress_container {
  width: 100%;
  vertical-align: middle;
  position: relative;
  margin-bottom: 48rpx;
}

.tp_progress_bg {
  width: 100%;
  position: absolute;
  padding: 0 173rpx;
  box-sizing: border-box;
  height: 24rpx;
}

.tp_progress_progress {
  width: 100%;
  position: absolute;
  padding: 0 173rpx;
  box-sizing: border-box;
  height: 20rpx;
}

.tp_progress_text {
  font-size: 22rpx;
  color: white;
}
